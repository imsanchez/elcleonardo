  "bugs": {
    "url": "https://github.com/contentful-userland/gatsby-contentful-starter/issues"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "bootstrap": "^4.4.1",
    "fb": "^2.0.0",
    "firebase": "^7.14.3",
    "flickity": "^2.2.1",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-firebase": "^0.1.8",
